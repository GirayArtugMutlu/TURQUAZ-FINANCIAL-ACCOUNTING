    public TurqUser(String username, String usersPassword, String usersRealName, String usersDescription, String createdBy, Date creationDate, String updatedBy, Date updateDate, Set turqUserPermissions, Set turqUserGroups) {
        this.username = username;
        this.usersPassword = usersPassword;
        this.usersRealName = usersRealName;
        this.usersDescription = usersDescription;
        this.createdBy = createdBy;
        this.creationDate = creationDate;
        this.updatedBy = updatedBy;
        this.updateDate = updateDate;
        this.turqUserPermissions = turqUserPermissions;
        this.turqUserGroups = turqUserGroups;
    }

    /** default constructor */
    public TurqUser() {
    }

    public Integer getId() {
        return this.id;
    }

