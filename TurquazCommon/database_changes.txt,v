CREATE TABLE turq_engine_menu (
    id integer NOT NULL,
    menu_name character varying(100) NOT NULL,
    menu_image character varying(100),
    menu_type integer NOT NULL,
    menu_module_component integer NOT NULL,
    parent_id integer NOT NULL,
    CONSTRAINT turq_engine_menu_pkey PRIMARY KEY (id),
    CONSTRAINT menu_module FOREIGN KEY (menu_module_component) REFERENCES turq_module_components(id) ON UPDATE RESTRICT ON DELETE RESTRICT
);


