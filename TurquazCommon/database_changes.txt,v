CREATE TABLE public.turq_services
(
  id int4 NOT NULL,
  service_name varchar(250) NOT NULL,
  class_name varchar(250) NOT NULL,
  method_name varchar(250) NOT NULL,
  CONSTRAINT turq_services_pkey PRIMARY KEY (id),
  CONSTRAINT turq_services_service_name_key UNIQUE (service_name)
)
