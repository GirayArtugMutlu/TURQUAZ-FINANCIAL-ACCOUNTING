--------------------------30-06-2005------------------------------------------
CREATE TABLE turq_inventory_transaction_bills
(
  id int4 NOT NULL,
  bill_date date NOT NULL,
  bill_definition varchar(250) NOT NULL,
  bill_type int4 NOT NULL,
  engine_sequence int4 NOT NULL,
  bill_document_no varchar(25) NOT NULL,
  created_by varchar(25) NOT NULL,
  creation_date date NOT NULL,
  updated_by varchar(25) NOT NULL,
  last_modified date NOT NULL,
  warehouse_in int4 NOT NULL,
  warehouse_out int4 NOT NULL,
  CONSTRAINT turq_inventory_transaction_bills_pkey PRIMARY KEY (id),
  CONSTRAINT eng_seq_trans FOREIGN KEY (engine_sequence) REFERENCES turq_engine_sequences (id) ON UPDATE RESTRICT ON DELETE RESTRICT,
  CONSTRAINT warehouse_in FOREIGN KEY (warehouse_in) REFERENCES turq_inventory_warehouses (id) ON UPDATE RESTRICT ON DELETE RESTRICT,
  CONSTRAINT warehouse_out FOREIGN KEY (warehouse_out) REFERENCES turq_accounting_account_classes (id) ON UPDATE RESTRICT ON DELETE RESTRICT
) 
WITHOUT OIDS;
----------------------------------------------------------------------------------


