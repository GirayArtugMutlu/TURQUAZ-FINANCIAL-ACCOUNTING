    public static void PGupdateVersion077(TurqSetting setting)throws Exception
    {   
        setting.setDatabaseVersion("0.8.0");
        EngDALCommon.updateObject(setting);
    }
    
    public static void HSQLDBupdateVersion080(TurqSetting setting)throws Exception
    {
        Session session = EngDALSessionFactory.getSession();
        Statement stmt = session.connection().createStatement();
        String query=
        	"INSERT INTO TURQ_CASH_TRANSACTION_TYPES VALUES(7,'Kapali Satis Faturasi','admin','2004-12-12','admin','2004-12-12');"+
        	"INSERT INTO TURQ_CASH_TRANSACTION_TYPES VALUES(8,'Kapali Alis Faturasi','admin','2004-12-12','admin','2004-12-12')";
        stmt.execute(query);       
        setting.setDatabaseVersion("0.8.1");
        EngDALCommon.updateObject(setting);    	
    }
    
